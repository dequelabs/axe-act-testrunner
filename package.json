{
  "name": "axe-act-testrunner",
  "version": "1.0.0",
  "description": "axe-core testrunner using the ACT test case format",
  "main": "src/index.js",
  "scripts": {
    "dev": "node --inspect src/index.js",
    "build": "npm run format && npm run lint && node src/index.js",
    "lint": "eslint 'src/**/*.js'",
    "format": "prettier --write  *.js 'src/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dequelabs/axe-act-testrunner.git"
  },
  "keywords": [
    "axe-core",
    "auto-wcag",
    "act",
    "testrunner"
  ],
  "author": "Jey <jey.nandakumar@deque.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dequelabs/axe-act-testrunner/issues"
  },
  "homepage": "https://github.com/dequelabs/axe-act-testrunner#readme",
  "dependencies": {
    "axe-core": "^3.3.2",
    "axe-reporter-earl": "0.1.0-canary.be38c0de",
    "fs-extra": "^7.0.0",
    "testrunner": "git+https://github.com/act-rules/testrunner.git#master"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-prettier-standard": "^2.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "prettier": "^1.17.0"
  }
}
